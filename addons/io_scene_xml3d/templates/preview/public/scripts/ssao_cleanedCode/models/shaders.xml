<?xml version="1.0" encoding="UTF-8"?>
<xml3d xmlns="http://www.w3.org/1999/xhtml">

    <shader id="cornell_shader" script="urn:xml3d:shader:phong">
        <float3 name="specularColor">0 0 0</float3>
        <float name="ambientIntensity">0.5</float>
        <float3 name="diffuseColor">0.6 0.6 0.9</float3>
    </shader>

    <shader id="grey_shader" script="urn:xml3d:shader:phong">
        <float3 name="specularColor">0 0 0</float3>
        <float name="ambientIntensity">0.5</float>
        <float3 name="diffuseColor">0.7 0.7 0.7</float3>
    </shader>

    <shader id="box_shader" script="urn:xml3d:shader:phong">
        <float3 name="specularColor">0 0 0</float3>
        <float name="ambientIntensity">0.5</float>
        <float3 name="diffuseColor">0.5 0.2 0.2</float3>
    </shader>

</xml3d>
